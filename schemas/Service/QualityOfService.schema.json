{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "QualityOfService.schema.json",
  "title": "QualityOfService",
  "definitions": {
    "QualityOfService": {
      "$id": "#QualityOfService",
      "description": "This object represents expected quality of service in terms of quality key index (KQI or KPI) with their associated service level objective and specification",
      "type": "object",
      "properties": {
        "keyIndex": {
          "type": "string",
          "description": "The quality key index (KQI or KPI)"
        },
        "validFor": {
          "$ref": "../Common/TimePeriod.schema.json#TimePeriod",
          "description": "A valid duration of this object"
        },
        "serviceLevelObjective": {
          "description": "corresponding service level objective",
          "$ref": "../Service/ServiceLevelObjectiveRef.schema.json#ServiceLevelObjectiveRef"
        },
        "serviceLevelSpecification": {
          "description": "correponding service level specification",
          "$ref": "../Service/ServiceLevelSpecificationRef.schema.json#/definitions/ServiceLevelSpecificationRef"
        }
      },
      "allOf": [
        {
          "$ref": "../Common/Extensible.schema.json#/definitions/Extensible"
        }
      ],
      "required": [
        "keyIndex",
        "serviceLevelSpecification"
      ]
    }
  }
}
