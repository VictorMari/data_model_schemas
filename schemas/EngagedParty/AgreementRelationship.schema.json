{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "AgreementRelationship.schema.json",
  "title": "AgreementRelationship",
  "definitions": {
    "AgreementRelationship": {
      "$id": "#AgreementRelationship",
      "description": "A uni-directionmal relationship from this agreement to another agreement",
      "type": "object",
      "properties": {
        "agreement": {
          "description": "The agreement being referred to",
          "$ref": "../EngagedParty/AgreementRef.schema.json#/definitions/AgreementRef"
        },
        "role": {
          "type": "string",
          "description": "The association role for this agreement"
        },
        "relationshipType": {
          "type": "string",
          "description": "Type of relationship such as containment, substitution, dependency, exclusivity"
        },
        "validFor": {
          "$ref": "../Common/TimePeriod.schema.json#TimePeriod",
          "description": "The period for which the relationship is valid"
        }
      },
      "allOf": [
        {
          "$ref": "../Common/Extensible.schema.json#/definitions/Extensible"
        }
      ]
    }
  }
}
