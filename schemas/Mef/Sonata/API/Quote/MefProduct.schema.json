{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "MefProduct.schema.json",
    "title": "MefProduct",
    "definitions": {
      "MefProduct": {
        "$id": "#MefProduct",
        "type": "object",
        "description": "One or more services sold to a Buyer by a Seller.  A particular Product Offering defines the technical and commercial attributes and behaviors of a Product.",
        "properties": {
            "place": {
                "type": "array",
                "items": {
                  "$ref": "../Common/MefRelatedPlaceRefOrValue.schema.json#MefRelatedPlaceRefOrValue"
                }
              },
              "productConfiguration": {
                "description": "MEFProductConfiguration is used to get MEF specific product/service payload. the @type is used a a discriminator.",
                "$ref": "../Common/MefProductConfiguration.schema.json#MefProductConfiguration"
              },
              "productOffering": {
                "$ref": "../Common/MefProductOfferingRef.schema.json#MefProductOfferingRef"
              },
              "productRelationship": {
                "type": "array",
                "items": {
                  "$ref": "../Common/MefProductRelationship.schema.json#MefProductRelationship"
                }
              }
        },
        "allOf": [
          {
            "$ref": "../../../../Gc/Product/GcProduct.schema.json#GcProduct"
          }
        ]
      }
    }
  }
  