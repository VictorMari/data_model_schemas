{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "ItemActionType.schema.json",
    "title": "ItemActionType",
    "definitions": {
      "ItemActionType": {
        "$id": "#ItemActionType",
        "description": "action to be performed on the entity managed by the item",
        "type": "string",
        "enum": [
          "add",
          "modify",
          "delete",
          "noChange"
        ]
      }
    }
  }