{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "GcQuoteItemRef.schema.json",
    "title": "GcQuoteItemRef",
    "definitions": {
        "GcQuoteItemRef": {
            "$id": "#GcQuoteItemRef",
            "type": "object",
            "description": "A reference to Quote item that has been executed previously.",
            "required": [
                "quoteItemId",
                "quoteId"
            ],
            "properties": {
                "quoteId": {
                    "type": "string",
                    "description": "Unique identifier of a refered quote."
                },
                "quoteItemId": {
                    "type": "string",
                    "description": "Identifier of a quote item. "
                }
            }
        }
    }
}