{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "GcStatusChange.schema.json",
    "title": "GcStatusChange",
    "definitions": {
        "GcStatusChange": {
            "$id": "#GcStatusChange",
            "type": "object",
            "required": [
                "status",
                "changeDate"
            ],
