          "items": {
            "$ref": "../DataGovernance/MetadataAssetRef.schema.json#MetadataAssetRef"
          }
        },
        "termOrCondition": {
          "type": "array",
          "items": {
            "$ref": "../EngagedParty/AgreementTermOrCondition.schema.json#AgreementTermOrCondition"
          }
        }
      },
      "allOf": [
        {
          "$ref": "../Common/Entity.schema.json#Entity"
        }
      ]
    }
  }
}